{
  "name": "@mdepascale/sveltekit-adapter-aws",
  "description": "SvelteKit to deploy your project using AWS CDK.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Depaa/sveltekit-adapter-aws.git"
  },
  "bugs": {
    "url": "https://github.com/Depaa/sveltekit-adapter-aws/issues"
  },
  "homepage": "https://github.com/Depaa/sveltekit-adapter-aws#readme",
  "license": "MIT",
  "keywords": [
    "sveltekit",
    "adapter",
    "aws",
    "cdk"
  ],
  "author": "Matteo Depascale",
  "version": "1.0.0",
  "main": "./index.js",
  "types": "./index.d.ts",
  "bin": {
    "destroy": "bin/destroy.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "format": "prettier --write .",
    "deploy": "npm version patch --no-git-tag-version && npm run build && npm publish --access public"
  },
  "devDependencies": {
    "@types/fs-extra": "11.0.1",
    "@types/node": "20.2.5",
    "@types/node-fetch": "2.6.4",
    "esbuild": "0.17.19",
    "prettier": "2.8.8",
    "typescript": "5.1.3"
  },
  "dependencies": {
    "@aws-cdk/aws-apigatewayv2-alpha": "2.82.0-alpha.0",
    "@aws-cdk/aws-apigatewayv2-integrations-alpha": "2.82.0-alpha.0",
    "@sveltejs/kit": "1.20.1",
    "aws-cdk": "2.82.0",
    "aws-cdk-lib": "2.82.0",
    "constructs": "10.2.43",
    "dotenv": "16.1.4",
    "fs-extra": "11.1.1",
    "svelte": "3.59.1",
    "update-dotenv": "1.1.1",
    "vite": "4.3.9"
  }
}
